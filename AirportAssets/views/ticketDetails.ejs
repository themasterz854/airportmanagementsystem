<body>
    <nav class="navbar navbar-inverse" style="border-radius:0px !important; margin:0;border: 0">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="index.html">Airline-X</a>
            </div>
            <div class="collapse navbar-collapse" id="myNavbar">
                <ul class="nav navbar-nav">
                    <li><a href="/homeUser">Home</a></li>
                    <li><a href="/booking">Booking</a></li>
                    <li class = "active"><a href="/ticketDetails">Ticket Details</a></li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="../index.html"><span class="glyphicon glyphicon-log-in"></span> Logout</a></a></li>
                </ul>


                <!-- Search bar -->
                <!-- <form class="navbar-form navbar-right">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="Search">
                        <div class="input-group-btn">
                            <button class="btn btn-default" type="submit">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                    </div>
                </form> -->
            </div>
        </div>
    </nav>


    <div class="background-wrapper" style="background-image: url(/254361.jpg);">

        <!--header-->


        <div class="container-fluid">
            <header>
                <h2>After Payement</h2>
                <hr>
            </header>
            <div class="col-sm-9 col-sm-offset-1">
                <div class="panel-group">
                    <div class="panel panel-default panel-transparent">
                        <div class="panel-heading">
                            <legend style="text-align: center;">Ticket Details</legend>
                        </div>
                        <div class="panel-body">
                            <table class="table table-hover" style="color: black; font-size: 20px">
                                <thead>
                                    <tr>
                                        <th>S. no</th>
                                        <th>FlightCode</th>
                                        <th>Ticket Number</th>
                                        <th>From</th>
                                        <th>To</th>
                                        <th>Date of booking</th>
                                        <th>Date of travel</th>
                                        <th>Departure Time</th>
                                        <th>Date of Cancellation</th>
                                        <th>Class</th>
                                        <th>Seat No</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <%
                                    if(TicketData.length!=0){
                                    var i=1;
                                    TicketData.forEach(function(data){
                                    %>
                                    <tr>
                                        <td><%=i; %></td>
                                        <td><%=data.FC %></td>
                                        <td><%=data.Ticket_Number %></td>
                                        <td><%=data.Source %></td>
                                        <td><%=data.Destination %></td>
                                        <td><%=data.DOBOOK %></td>
                                        <td><%=data.DOT %></td>
                                        <td><%=data.DepartureTime %></td>
                                        <td><%=data.Date_of_Cancellation %></td>
                                        <td><%=data.Class %></td>
                                        <td><%=data.SeatNo %></td>
                                        
                                    </tr>
                                    <%  i++; }) %>
                                    <% } else{ %>
                                        <tr>
                                            <td colspan="7">No Data Found</td>
                                        </tr>
                                    <% } %>
                                    </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
